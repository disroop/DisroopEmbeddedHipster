{
    "extends": [
      "config:base",
      "docker:pinDigests"
    ],
   "packageRules": [
      {
        "updateTypes": ["minor", "patch", "pin", "digest"],
      }
    ],
    "regexManagers": [
      {
        "fileMatch": ["^.devcontainer.json$"],
        "matchStrings": ["(?<depName>disroop/embedded-hipster-dev):(?<currentValue>[0-9.]+)\n"],
        "datasourceTemplate": "docker"
      }
    ]
  }