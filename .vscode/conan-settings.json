{
    "profiles": [
        { 
            "name":"blinkylinux", 
            "conanFile":"${workspaceFolder}/blinky/conanfile.py",
            "profile":"clang-linux-debug",
            "installArg": "--build missing -o run_cpptest_pro=True",
            "buildArg":"",
            "createUser": "disroop",
            "createChannel": "development",
            "createArg": "--build=missing -o run_cpptest_pro=True" 
        }
    ],
    "workspace":[
        {
            "name":"ws-nrf",
            "conanWs": "${workspaceFolder}/.workspace/ws-blinky-nrf.yml",
            "profile":"gcc-nrf52840-debug",
            "arg": "--build=missing --settings os.board=NRF52_DK -e DISROOP_BOARD=NRF52_DK"
        },
        {
            "name":"ws-linux",
            "conanWs": "${workspaceFolder}/.workspace/ws-blinky-linux.yml",
            "profile":"clang-linux-debug",
            "arg": "--build=missing"
        }
    ]
}